{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "google",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "max-len": [
            "error",
            {
                "code": 80,
                "tabWidth": 4,
                "ignoreUrls": true,
                "ignoreStrings": true,
                "ignorePattern": ".(module|require)'",
                "ignoreComments": true
            }
        ]
    }
}
